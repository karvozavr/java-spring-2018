<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane minHeight="800.0"
            minWidth="600.0"
            prefHeight="800.0"
            prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ru.spbau.mit.karvozavr.tictactoe.ui.TicTacToeApp">
    <stylesheets>
        <URL value="@styles/styles.css"/>
    </stylesheets>

    <children>
        <GridPane alignment="CENTER" layoutX="225.0" layoutY="125.0" AnchorPane.bottomAnchor="0.0"
                  AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="50.0" prefWidth="200.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="50.0" prefWidth="200.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="50.0" prefWidth="200.0"/>

            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="200.0" minHeight="50.0" prefHeight="200.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="200.0" minHeight="50.0" prefHeight="200.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="200.0" minHeight="50.0" prefHeight="200.0" vgrow="SOMETIMES"/>
            </rowConstraints>

            <children>
                <Button styleClass="field-cell-button" text="Button1" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
                <Button styleClass="field-cell-button" text="Button2" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
                <Button styleClass="field-cell-button" text="Button3" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
                <Button styleClass="field-cell-button" text="Button4" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
                <Button styleClass="field-cell-button" text="Button5" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Button styleClass="field-cell-button" text="Button6" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <Button styleClass="field-cell-button" text="Button7" GridPane.columnIndex="2" GridPane.rowIndex="0"/>
                <Button styleClass="field-cell-button" text="Button8" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
                <Button styleClass="field-cell-button" text="Button9" GridPane.columnIndex="2" GridPane.rowIndex="2"/>
            </children>
        </GridPane>
    </children>
</AnchorPane>
